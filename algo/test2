const students = [
  {
    name: "John",
    chemistryMarks: 85,
    biologyMarks: 90,
    dob: "12-02-1998",
  },
  {
    name: "Jane",
    chemistryMarks: 80,
    biologyMarks: 95,
    dob: "15-07-1997",
  },
  {
    name: "Alice",
    chemistryMarks: 85,
    biologyMarks: 90,
    dob: "22-01-1998",
  },
];

// Function to compare students based on the sorting rules
function compareStudents(student1, student2) {
  if (student1.chemistryMarks + student1.biologyMarks >
      student2.chemistryMarks + student2.biologyMarks) {
    return -1;
  } else if (student1.chemistryMarks + student1.biologyMarks <
             student2.chemistryMarks + student2.biologyMarks) {
    return 1;
  }

  // Second priority: Biology marks
  if (student1.biologyMarks > student2.biologyMarks) {
    return -1;
  } else if (student1.biologyMarks < student2.biologyMarks) {
    return 1;
  }

  // Third priority: Date of birth (no specific sorting needed)

  // If all conditions are equal, maintain the original order
  return 0;
}

// Sort the array of students using the compare function
students.sort(compareStudents);

// Print the sorted array of students
students.forEach((student) => {
  console.log(student);
});
